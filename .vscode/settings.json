{
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-shell-escape",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
        },
        {
            "name": "mylatexformat",
            "command": "etex",
            "args": [
                "-initialize",
                "-jobname=%DOCFILE%",
                "&pdflatex",
                "mylatexformat.ltx",
                "\"\"\"%DOCFILE%\"\"\""
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        },{
            "name": "recompile header",
            "tools": [
                "mylatexformat"
            ]
        }
    ],
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./project1",
        "-p",
        "*test.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
}